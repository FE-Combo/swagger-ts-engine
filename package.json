{
  "name": "swagger-ts-engine",
  "version": "0.1.0",
  "description": "Automatically generate service & type based on swagger",
  "main": "src/index.js",
  "scripts": {
    "api": "node src/test.js",
    "prettier": "prettier --write '**/*.{ts,js,md,json}'",
    "release": "standard-version",
