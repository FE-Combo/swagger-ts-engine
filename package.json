    "@types/node": "^14.14.22",
    "prettier": "^2.2.1",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "chalk": " ^4.1.0",
    "fs-extra": "^9.1.0"
  }
